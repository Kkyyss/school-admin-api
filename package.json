{
  "name": "school-admin-api",
  "version": "1.0.0",
  "description": "Backend API for student registration and management",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/env-cmd -e development node index.js",
    "start:dev": "./node_modules/.bin/env-cmd -e development node index.js",
    "start:local": "./node_modules/.bin/env-cmd -e local node index.js",
    "test:dev": "./node_modules/.bin/env-cmd -e test,development ./node_modules/mocha/bin/mocha -S --no-timeouts ./tests",
    "test:local": "./node_modules/.bin/env-cmd -e test,local ./node_modules/mocha/bin/mocha -S --no-timeouts ./tests",
    "eslint:fix": "./node_modules/.bin/eslint --fix --ignore-path .gitignore ./tests .",
    "eslint:test": "eslint --ignore-path .gitignore .",
    "prettier": "./node_modules/.bin/prettier --single-quote --trailing-comma es5 --print-width 80 --write --no-bracket-spacing **/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kkyyss/school-admin-api.git"
  },
  "keywords": [
    "nodeJs",
    "mysql",
    "sequelize"
  ],
  "author": "Ong Kang Yi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kkyyss/school-admin-api/issues"
  },
  "homepage": "https://github.com/Kkyyss/school-admin-api#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "email-validator": "^2.0.4",
    "env-cmd": "^10.0.1",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "helmet": "^3.21.0",
    "morgan": "^1.9.1",
    "mysql2": "^1.7.0",
    "sequelize": "^5.18.4"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "6.4.0",
    "eslint-config-strongloop": "2.1.0",
    "mocha": "^6.2.0",
    "prettier": "1.18.2"
  }
}
